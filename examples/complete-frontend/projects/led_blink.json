{
  "name": "LED Blink",
  "description": "Simple LED blinking pattern using GPIO pin 18",
  "version": "1.0",
  "blocks": [
    {
      "id": "start_1",
      "type": "start",
      "position": { "x": 50, "y": 100 },
      "params": {
        "auto_start": true
      }
    },
    {
      "id": "loop_1",
      "type": "loop",
      "position": { "x": 200, "y": 100 },
      "params": {
        "count": 10,
        "infinite": false
      }
    },
    {
      "id": "gpio_on",
      "type": "gpio-output",
      "position": { "x": 350, "y": 80 },
      "params": {
        "pin": 18,
        "value": "1",
        "duration": 0
      }
    },
    {
      "id": "delay_on",
      "type": "delay",
      "position": { "x": 500, "y": 80 },
      "params": {
        "duration": 500
      }
    },
    {
      "id": "gpio_off",
      "type": "gpio-output",
      "position": { "x": 650, "y": 80 },
      "params": {
        "pin": 18,
        "value": "0",
        "duration": 0
      }
    },
    {
      "id": "delay_off",
      "type": "delay",
      "position": { "x": 800, "y": 80 },
      "params": {
        "duration": 500
      }
    },
    {
      "id": "log_1",
      "type": "log",
      "position": { "x": 350, "y": 150 },
      "params": {
        "message": "LED blink cycle completed",
        "level": "info"
      }
    }
  ],
  "connections": [
    {
      "id": "conn_1",
      "from": { "blockId": "start_1", "output": "trigger" },
      "to": { "blockId": "loop_1", "input": "trigger" }
    },
    {
      "id": "conn_2",
      "from": { "blockId": "loop_1", "output": "iteration" },
      "to": { "blockId": "gpio_on", "input": "trigger" }
    },
    {
      "id": "conn_3",
      "from": { "blockId": "gpio_on", "output": "complete" },
      "to": { "blockId": "delay_on", "input": "trigger" }
    },
    {
      "id": "conn_4",
      "from": { "blockId": "delay_on", "output": "complete" },
      "to": { "blockId": "gpio_off", "input": "trigger" }
    },
    {
      "id": "conn_5",
      "from": { "blockId": "gpio_off", "output": "complete" },
      "to": { "blockId": "delay_off", "input": "trigger" }
    },
    {
      "id": "conn_6",
      "from": { "blockId": "delay_off", "output": "complete" },
      "to": { "blockId": "log_1", "input": "trigger" }
    }
  ],
  "hardware": [
    {
      "type": "gpio",
      "pin": 18,
      "mode": "output",
      "description": "LED connected to GPIO pin 18"
    }
  ],
  "created_at": "2024-01-01T00:00:00Z",
  "updated_at": "2024-01-01T00:00:00Z"
}
